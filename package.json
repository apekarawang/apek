    "vue-script2": "^2.0.3",
    "vuepress": "^0.14.2",
    "vuera": "^0.2.2",
    "vuetify": "^1.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-rc.2",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-imports": "^1.5.0",
