    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.0",
    "@mdi/svg": "^2.6.95",
    "cross-env": "^5.0.1",
    "es6-promise": "^4.2.4",
    "material-icons": "^0.2.1",
    "vue-mdc-adapter": "^0.18.2",
    "vue-recaptcha": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "cp": "^0.2.0",
    "eslint": "^4.15.0",
