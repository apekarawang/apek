{
  "name": "apek",
  "version": "1.0.0",
  "description": "Asosiasi Pengusaha Engineering Karawang",
  "author": {
    "email": "ekoeryanto@gmail.com",
    "name": "Eko Eryanto"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekoeryanto/apek.git"
  },
  "scripts": {
    "start": "vuepress dev docs",
    "build:docs": "vuepress build docs",
    "build": "npm-run-all build:*",
    "build:cms": "webpack --mode=production",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --ignore-path .gitignore --parser vue --single-quote --trailing-comma es5 --write",
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "git add",
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@pake/react-color": "^3.0.0",
    "netlify-cms": "^2.0.9",
    "netlify-cms-widget-color": "^3.0.0",
    "netlify-cms-widget-material-icons": "^3.0.0",
    "react-virtualized-select": "^3.1.3",
    "vue-recaptcha": "^1.1.1",
    "vuepress": "^0.14.2",
    "vuera": "^0.2.2",
    "vuetify": "^1.2.0-beta.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-rc.2",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "cp": "^0.2.0",
    "css-to-string-loader": "^0.1.3",
    "eslint": "^4.15.0",
    "eslint-plugin-vue": "^4.0.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.22.1",
    "to-string-loader": "^1.1.5",
    "webpack-cli": "^3.1.0"
  }
}
