    "*.{png,jpeg,jpg,gif,svg}": [
      "git add",
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
